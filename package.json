{
  "name": "rest-store",
  "version": "0.0.1",
  "description": "Sync your client-side state store with REST APIs",
  "main": "dist/index.js",
  "scripts": {
    "start": "webpack --devtool source-map --progress --colors --watch",
    "build": "webpack --devtool source-map --progress --colors",
    "pretest": "standard",
    "mocha": "mocha --compilers js:babel-register --reporter spec --colors test/**",
    "test": "cross-env NODE_ENV=test nyc -r lcov -r text npm run mocha"
  },
  "nyc": {
    "include": [
      "src/*.js",
      "index.js"
    ],
    "sourceMap": false,
    "instrument": false
  },
  "standard": {
    "ignore": [
      "/dist"
    ]
  },
  "peerDependencies": {
    "cerebral-model-baobab": "^0.4.9"
  },
  "dependencies": {
    "axios": "^0.9.1",
    "node-uuid": "^1.4.7",
    "path-to-regexp": "^1.2.1",
    "url-parse": "^1.1.1"
  },
  "devDependencies": {
    "babel-core": "^6.4.0",
    "babel-loader": "^6.2.1",
    "babel-plugin-__coverage__": "^11.0.0",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-stage-0": "^6.3.13",
    "babel-register": "^6.4.3",
    "chai": "^3.4.1",
    "cross-env": "^1.0.8",
    "mocha": "^2.3.4",
    "nock": "^7.2.2",
    "nyc": "^6.6.1",
    "sinon": "^1.17.4",
    "standard": "^7.1.2",
    "webpack": "^1.12.11"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/bfitch/rest-store"
  },
  "keywords": [
    "cerebral",
    "ajax",
    "baobab",
    "REST"
  ],
  "author": "Brian Fitch",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/bfitch/rest-store/issues"
  },
  "homepage": "https://github.com/bfitch/rest-store"
}
